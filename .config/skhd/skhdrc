# focus window
cmd - h : yabai -m window --focus west
cmd - j : yabai -m window --focus north
cmd - k : yabai -m window --focus south
cmd - l : yabai -m window --focus east

# move space
cmd + ctrl - h : yabai -m space --move prev
cmd + ctrl - l : yabai -m space --move next

# move space to window
cmd + shift - h : yabai -m window --space prev
cmd + shift - l : yabai -m window --space next

# change window mode
cmd + ctrl - f : yabai -m window --toggle float
