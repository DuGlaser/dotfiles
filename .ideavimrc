set surround
set clipboard+=unnamed
set commentary
set hlsearch

nnoremap <C-h> <C-w>h
nnoremap <C-j> <C-w>j
nnoremap <C-k> <C-w>k
nnoremap <C-l> <C-w>l
nnoremap <Space>l :nohlsearch<CR>

nnoremap <Space>rn :action RenameElement<CR>

nnoremap zl zo
nnoremap zL zO
nnoremap zh zc
nnoremap zH zC

Plug 'preservim/nerdtree'
nnoremap <Space>u :NERDTree<CR>
nnoremap <Space>o :NERDTreeClose<CR>
